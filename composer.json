{
    "name": "bristol-su/portal",
    "type": "project",
    "description": "Bristol SU Portal.",
    "keywords": [
    ],
    "license": "MIT",
    "prefer-stable": false,
    "require": {
        "php": "^7.2",
        "aws/aws-sdk-php": "^3.88",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "^6.5",
        "laravel/passport": "^8.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "php-console/php-console": "^3.1",
        "bristol-su/support": "dev-master",
        "bristol-su/upload-file": "@dev",
        "bristol-su/typeform": "@dev",
        "bristol-su/control-db-implementation": "@dev"
    },
    "require-dev": {
        "itsgoingd/clockwork": "^4",
        "beyondcode/laravel-dump-server": "^1.0",
        "bmitch/churn-php": "^1.0",
        "composer/composer": "^1.8",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../modules/typeform"
        },
        {
            "type": "path",
            "url": "../modules/upload-file"
        },
        {
            "type": "path",
            "url": "../support/support"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/passport"
            ]
        }
    }
}
